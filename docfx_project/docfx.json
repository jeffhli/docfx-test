{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**/*.csproj"
          ]
        }
      ],
      "dest": "api/v2",
      "includePrivateMembers": false,
      "disableGitFeatures": true,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "EnumSortOrder": "alphabetic",
      "allowCompilationErrors": false,
      "properties": {
        "TargetFramework": "net6.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/v1/*.yml"
        ],
        "group": "1.1"
      },
      {
        "files": [
          "api/v2/*.yml"
        ],
        "group": "2.1"
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}